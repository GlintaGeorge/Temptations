        </div>
    </section>
    <section class="product-tabs section-padding wow fadeIn animated">
        <div class="container">
            <div class="tab-header">
                <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item" role="presentation">
                        <button class="nav-link active" id="nav-tab-one" data-bs-toggle="tab" data-bs-target="#tab-one"
                            type="button" role="tab" aria-controls="tab-one" aria-selected="true">Best Seller's</button>
                    </li>
                    
                </ul>
                <a href="#" class="view-more d-none d-md-flex">View More<i
                        class="fi-rs-angle-double-small-right"></i></a>
            </div>
            <!--End nav-tabs-->

            <div class="tab-content wow fadeIn animated" id="myTabContent">
                <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">
                    <div class="row product-grid-4">

                        <% if (products.length>0) { %>

                            <% products.forEach(element=> { %>

                        <div class="col-lg-4 col-md-4 col-12 col-sm-6">
                            

                                    <div class="product-cart-wrap mb-30">
                                        <div class="product-img-action-wrap">

                                            <% if (Array.isArray(element.images) && element.images.length> 0) { %>



                                                <div class="product-img product-img-zoom">


                                                    <a href="/Products/<%= element?._id %>">
                                                        <img src="<%= element.images[0].imageUrl %>" alt="imagepicture">
                                                    </a>
                                                </div>

                                                <% } %>

                                                    

                                                    <div
                                                        class="product-badges product-badges-position product-badges-mrg">
                                                        <span class="hot">Hot</span>
                                                    </div>
                                        </div>
                                        <div class="product-content-wrap">
                                            <!-- <div class="product-category">
                                                <a href="/Products/<%= element.categoryName%>"><%= element.categoryName %></a>
                                            </div> -->
                                            <h4><b><a href="/Products/<%= element._id%>">
                                                        <%= element.title %>
                                                    </a></b></h4>

                                            <!-- <div class="rating-result" title="90%">
                                                <span>
                                                    <span>90%</span>
                                                </span>
                                            </div> -->
                                            <!-- <div class="product-price">
                                                <span>₹<%= (element.salePrice).toLocaleString() %> </span>
                                                <span class="old-price">₹<%=( element.productPrice).toLocaleString() %>
                                                        </span>
                                            </div> -->
                                           
                                                        <div class="product-action-1 show">

                                                        </div>
                                        </div>
                                    </div>
                        </div>
                        <% }); %>
                        <% } else { %>
                            <h2>No Products</h2>
                            <% } %>


                        <!--End product-grid-4-->

                    </div>
                              
                </div>

                <!--En tab one (Featured)-->

                <!--En tab two (Popular)-->

                <!--En tab three (New added)-->
            </div>
            <!--End tab-content-->
        </div>
    </section>
